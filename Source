hSlider7:
SetTimer,Adjust_Slider7,0
fn7 := Func("MouseMove7").Bind(SliderBackground7,SliderHandle7)
OnMessage(0x201,fn7)
return
Adjust_Slider7:
if(GetKeyState("LButton")){
CoordMode,Mouse,Client
MouseGetPos,ty
(ty<87)?(ty:=87):(ty>187)?(ty:=187)
ButtonValue7:=((ty-87)*+3)-0, Active7:=1
GuiControl,,% SliderBackground7,% ButtonValue7
GuiControl,MoveDraw,% SliderHandle7,%  "x" ty
GuiControl,,% DisplayText7,% ButtonValue7
}else   {
SetTimer,Adjust_Slider7,Off
Active7:=0
return
}
return
MouseMove7(SliderBackground7,SliderHandle7) {
MouseGetPos,,,,ctrl,2
if(!Active&&(ctrl=SliderBackground7||ctrl=SliderHandle7))
SetTimer,Adjust_Slider7,0
}
return
