hSliderMZ:
        SetTimer,Adjust_SliderMZ,0
fnMZ := Func("MouseMoveMZ").Bind(SliderBackgroundMZ,SliderHandleMZ)
OnMessage(0x201,fnMZ)
return
Adjust_SliderMZ:
    if(GetKeyState("LButton")){
       
        CoordMode,Mouse,Client   

        MouseGetPos,ty
        
       (ty<86)?(ty:=86):(ty>186)?(ty:=186)

        ButtonValueMZ:=((ty-86)*+1)-0, ActiveMZ:=1
        
        GuiControl,,% SliderBackgroundMZ,% ButtonValueMZ
        
        GuiControl,MoveDraw,% SliderHandleMZ,%  "x" ty
        
        GuiControl,,% DisplayTextMZ,% ButtonValueMZ
        
    }else   {
        
        SetTimer,Adjust_SliderMZ,Off
        
        ActiveMZ:=0
        
        return
        
    }
    return

MouseMoveMZ(SliderBackgroundMZ,SliderHandleMZ) { 
    
    MouseGetPos,,,,ctrl,2
    
    if(!Active&&(ctrl=SliderBackgroundMZ||ctrl=SliderHandleMZ))
        
        SetTimer,Adjust_SliderMZ,0
        
}
return

